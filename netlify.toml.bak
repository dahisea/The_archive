[build]
publish = "public"
command = "npm install --save iamtraction/google-translate && npm install axios && node processUsers.js && hugo --gc --minify --enableGitInfo"

[context.production.environment]
HUGO_VERSION = "0.130.0" #请根据自己的 Hugo 版本进行修改
HUGO_ENV = "production"
HUGO_ENABLEGITINFO = "true"

[context.split1]
command = "npm install --save iamtraction/google-translate && npm install axios && node processUsers.js && hugo --gc --minify --enableGitInfo"

[context.split1.environment]
HUGO_VERSION = "0.130.0" #请根据自己的 Hugo 版本进行修改
HUGO_ENV = "production"
HUGO_ENABLEGITINFO = "true"
